                {
                    content: "Open preferences / profile screen",
                    trigger: "[data-menu=settings]",
                    run: "click",
                },
                {
                    content: "Update the email address",
                    trigger: 'div[name="email"] input',
                    run: "edit updatedemail@example.com",
